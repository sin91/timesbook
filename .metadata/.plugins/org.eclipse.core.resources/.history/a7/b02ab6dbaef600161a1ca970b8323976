<mapper namespace="org.mybatis.example.BlogMapper">
  <resultMap type="" id="categoryResultMap"></resultMap>
  <select id="selectBlog" resultType="Blog">
    SELECT CATEGORY_ID,
    	   CATEGORY_NAME
      FROM CATEGORY
     WHERE CATEGORY_ID=${id}
  </select>
</mapper>